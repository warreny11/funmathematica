sol1 = DSolve[{v'[t] == (P1*A1)/M0 - g, v[0] == 0}, v[t], t]
v1[t_] = v[t] /. sol1[[1]]
h1[t_] = Integrate[v1[t], t]
a1[t_] = D[v1[t], t]

t1 = Solve[h1[t] == L, t]
vi1 = Simplify[v1[t] /. t1[[2]]]

L = .22
A1 = ((4.75*10^(-3))^2)*Pi
P1 = 4.4*10^5
M0 = 0.6889
g = 9.81

sol2 = DSolve[{v'[t] == (2*A1(P1-P2))/(M0-œÅ1*ve*A1*t)-g, v[0] == 3.95012}, v[t], t]
v2[t_] = v[t] /. sol2[[1]]
h2[t_] = Integrate[v2[t], t]
a2[t_] = D[v2[t], t]



t2 = Solve[M[t] == 0, t]
vi2 = Simplify[v2[t] /. t2[[1]]]


A1 = ((4.75*10^(-3))^2)*Pi
M0 = 0.6889
ve = 26.07680962
\[Rho]1 = 1000


sol3 = DSolve[{v'[t] == -g - (k*c*(v[t])^(2))/(M2), v[0] == 41.0204}, v[t],
   t]
v3[t_] = v[t] /. sol3[[1]]
h3[t_] = Integrate[v3[t], t]
a3[t_] = D[v3[t], t]


Clear[A1, M0, ve]

t3 = Solve[v3[t] == 0, t]
vi3 = Simplify[v3[t] /. t3[[1]]]

