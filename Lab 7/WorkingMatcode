Plot[h4[t] - h4[0], {t, 0, 5}]

Plot[v4[t], {t, 0, 10.91}]

Plot[a4[t], {t, 0, 10.91}]

t1 = 0.111389
t2 = t1 + 0.2705069712
t3 = t2 + 2.14934
t4 = t3 + 10

hcom[t_] := h1[t] /; t < t1 
hcom[t_] := h2[t] /; t1 < t < t2
hcom[t_] := h3[t] /; t2 < t < t3
hcom[t_] := h4[t] /; t3 < t < t4

Plot[hcom[t], {t, 0, 10}]