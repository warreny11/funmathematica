Plot[a1[t], {t, 0, 10.91}]


Plot[h2[t], {t, 0, 10.91}]

Plot[v2[t], {t, 0, 10.91}]

Plot[a2[t], {t, 0, 10.91}]


Plot[h3[t], {t, 0, 10.91}]

