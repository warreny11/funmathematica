  t]
v4[t_] = v[t] /. sol4[[1]]
h4[t_] = Integrate[v4[t], t]
a4[t_] = D[v4[t], t]

t4 = Solve[h4[t] == 0, t]
vi4 = Simplify[h4[t] /. t4[[2]]]


